import java.util.Scanner;
public class HW1 {


	public static void main(String[] arg)
	{
		Scanner Keyboard = new Scanner(System.in);
		int roll_num;
		String name,address,course;
		

		System.out.println("Input your roll number: ");
		roll_num=Keyboard.nextInt();
		Keyboard.next();
		System.out.println("Input your name: ");
		name=Keyboard.nextLine();

		System.out.println("Input your address: ");
		address=Keyboard.nextLine();

		System.out.println("Input your the course: ");
		course=Keyboard.nextLine();


		System.out.println(name+" has roll no "+roll_num);
		System.out.println("his address is "+address+"and the course he studies is"+course);
		
	}
	
	
	
	
}
