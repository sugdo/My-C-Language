import java.util.Scanner;

public class go {

	public static void main(String[] arg)
	{
		double score;
		String grade;
		Scanner Keyboard = new Scanner(System.in);
		
		System.out.println("Enter your grade: ");
		
		grade = Keyboard.next();
		
		switch(grade)
		{
		
		case A:
		
			score=4.0;
			break;
		
		case B:
		
			score=3.0;
			break;
		
		case C:
		
			score=2.0;
			break;
		
		case D:
		
			score=1.0;
			break;
		
		default :
			score=0;
		
		}
		
		System.out.println("Score="+score);
		System.out.println("Grade="+grade);
		
		
	}
	
	
}